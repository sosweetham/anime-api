{
    "name": "@anime-api/client",
    "version": "0.0.1",
    "devDependencies": {
        "@lucide/svelte": "^0.485.0",
        "@playwright/test": "^1.49.1",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.16.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.16",
        "autoprefixer": "^10.4.20",
        "bits-ui": "^1.3.14",
        "clsx": "^2.1.1",
        "mdsvex": "^0.12.3",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "tailwind-merge": "^3.0.2",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.0.0",
        "vite": "^6.0.0",
        "vitest": "^3.0.0"
    },
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && bunx @biomejs/biome check ./src",
        "check-fix": "bunx @biomejs/biome check --write ./src",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "test:e2e": "playwright test",
        "format": "npx @biomejs/biome format --write ./src",
        "check-format": "npx @biomejs/biome format ./src",
        "lint": "npx @biomejs/biome lint --write ./src",
        "check-lint": "npx @biomejs/biome lint ./src"
    },
    "type": "module"
}
